<?xml version="1.0" encoding="UTF-8"?>
<ServiceManifest Name="FrontendServicePackage" Version="1.0" xmlns="http://schemas.microsoft.com/2011/01/fabric" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Description>Service that implements a backend service</Description>
    <ServiceTypes>
        <StatelessServiceType ServiceTypeName="StatelessBackendService"  UseImplicitHost="true"/>
    </ServiceTypes>
    <CodePackage Name="BackendService.Code" Version="1.0">
        <EntryPoint>
          <ContainerHost>
            <ImageName>travisyeh/backend:latest</ImageName>
            <Commands></Commands>
          </ContainerHost>
        </EntryPoint>
      <EnvironmentVariables>
        <EnvironmentVariable Name="PublicIP" Value=""/>
        <EnvironmentVariable Name="HttpGatewayPort" Value=""/>
        <EnvironmentVariable Name="BackendServiceName" Value=""/>
      </EnvironmentVariables>
    </CodePackage>
    <ConfigPackage Name="BackendService.Config" Version="1.0" />
    <DataPackage Name="BackendService.Data" Version="1.0" />
  <Resources>
    <Endpoints>
      <Endpoint Name="Endpoint1" UriScheme="http" Protocol="tcp"/>
    </Endpoints>
  </Resources>
</ServiceManifest> 